{
  "name": "print-js",
  "homepage": "http://printjs.crabbly.com",
  "description": "A tiny javascript library to help printing from the web.",
  "version": "1.6.0",
  "type": "module",
  "main": "dist/print.js",
  "module": "dist/print.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/print.js",
      "require": "./dist/print.umd.cjs"
    },
    "./dist/print.css": "./dist/print.css"
  },
  "repository": "https://github.com/crabbly/Print.js",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^24.7.1",
    "@vitest/browser": "^2.1.9",
    "@vitest/coverage-v8": "^2.1.9",
    "coveralls": "^3.1.1",
    "jsdom": "^27.0.0",
    "playwright": "^1.49.1",
    "sass": "^1.83.0",
    "standard": "^17.1.2",
    "typescript": "^5.8.2",
    "vite": "^6.0.7",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^2.1.9"
  },
  "scripts": {
    "test": "standard && vitest run",
    "test:ui": "vitest --ui",
    "dev": "vite build --mode development",
    "watch": "vite build --watch",
    "production": "vite build",
    "build": "vite build",
    "coverage": "vitest run --coverage",
    "coveralls": "cat coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "start": "vite",
    "standard:fix": "standard --fix"
  },
  "author": "Rodrigo Vieira <rodrigo@crabbly.com>",
  "standard": {
    "ignore": [
      "/dist/print.js",
      "/test/**/*.spec.js"
    ],
    "env": {
      "browser": true,
      "jasmine": true
    }
  },
  "keywords": [
    "printjs",
    "print.js",
    "print-js"
  ]
}
